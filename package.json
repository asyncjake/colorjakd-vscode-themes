{
  "name": "colorjakd",
  "displayName": "colorjakd / jakethedev's theme pack",
  "description": "Several themes that I've made or forked, packaged conveniently for myself and anyone else that digs some color in their daily grind",
  "version": "1.0.0",
  "scripts": {
    "start": "echo Edit a theme, use 'npm run build' to generate the vsix file",
    "build": "vsce package && mv *.vsix builds/ && echo 'Build complete, check builds folder!'",
